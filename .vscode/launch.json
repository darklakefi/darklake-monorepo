{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "chrome",
      "request": "launch",
      "runtimeExecutable": "/usr/bin/brave-browser",
      "name": "brave",
      "url": "http://localhost:3000",
      "webRoot": "${workspaceFolder}/web",
      "userDataDir": false
    },
    {
      "type": "node",
      "request": "launch",
      "name": "anchor test",
      "skipFiles": ["<node_internals>/**"],
      "outFiles": ["${workspaceFolder}/dist/out-tsc/**/*.js"],
      "cwd": "${workspaceFolder}/anchor",
      "runtimeExecutable": "anchor",
      "runtimeArgs": ["test"],
      "sourceMaps": true,
      "resolveSourceMapLocations": ["${workspaceFolder}/**"],
      "console": "integratedTerminal"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "jest: generate valid proof",
      "cwd": "${workspaceFolder}/anchor",
      "runtimeExecutable": "${workspaceFolder}/node_modules/.bin/jest",
      "args": [
        "--config",
        "./jest.config.ts",
        "./tests/jest/groth16.spec.ts",
        "-t",
        "should generate a valid proof",
        "--",
        "--no-capture"
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen"
    }
  ]
}
